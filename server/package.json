{
  "name": "ebac-demo-store-server",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:watch": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "build": "nest build",
    "test": "jest",
    "seed": "ts-node scripts/seed.ts",
    "db:migrate-save": "prisma migrate dev",
    "db:migrate-up": "prisma migrate deploy",
    "db:clean": "ts-node scripts/clean.ts",
    "db:init": "run-s \"db:migrate-save -- --name 'initial version'\" db:migrate-up seed",
    "prisma:generate": "prisma generate",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:build": "docker build ."
  },
  "dependencies": {
    "@apollo/server": "^4.7.4",
    "@josephg/resolvable": "^1.0.1",
    "@nestjs/apollo": "^12.0.1",
    "@nestjs/common": "^10.0.2",
    "@nestjs/config": "",
    "@nestjs/core": "^10.0.2",
    "@nestjs/graphql": "^12.0.1",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.0.2",
    "@nestjs/serve-static": "^4.0.0",
    "@nestjs/swagger": "^7.0.2",
    "@prisma/client": "^4.15.0",
    "@types/bcrypt": "^5.0.0",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "graphql": "^16.6.0",
    "graphql-type-json": "^0.3.2",
    "nest-access-control": "^2.2.0",
    "nest-morgan": "^2.0.0",
    "nestjs-prisma": "^0.20.1",
    "npm-run-all": "^4.1.5",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.1.13",
    "swagger-ui-express": "^4.6.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.3",
    "@nestjs/testing": "^10.0.2",
    "@types/express": "^4.17.17",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^29.5.2",
    "@types/normalize-path": "^3.0.0",
    "@types/passport-http": "^0.3.9",
    "@types/passport-jwt": "^3.0.8",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "jest-mock-extended": "^3.0.4",
    "prisma": "^4.15.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "type-fest": "0.11.0",
    "typescript": "4.7.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "<rootDir>/dist/"
    ]
  }
}
